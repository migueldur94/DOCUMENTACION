======================== Rabit:
docker pull rabbitmq:3.8-management-alpine
docker run -d -p 15672:15672 -p 5672:5672 --name nexus33-microservice-rabbitmq38 -d rabbitmq:3.8-management-alpine


======================== postgresql:
docker pull postgres:12-alpine
docker run -p 5432:5432 --name nexus33-database-postgres12 -e POSTGRES_PASSWORD=Nexus33! -e POSTGRES_DB=nexus -d postgres:12-alpine 
docker exec -it -u root nexus33-database-postgres12 /bin/bash
psql -h localhost -p 5432 -U postgres -d nexus
